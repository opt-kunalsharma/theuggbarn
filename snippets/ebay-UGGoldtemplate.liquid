			<div class="tab">
				<input type="radio" id="tab-3" name="tab-group-1">
				<label for="tab-3">Warranty</label>

				<div class="content">
					{include field="WarrantyHTML"}
				</div>
			</div>
			{/if}

			<div class="tab">
				<input type="radio" id="tab-4" name="tab-group-1">
